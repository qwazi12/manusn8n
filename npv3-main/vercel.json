{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sfo1"],
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://nodepilot.dev",
    "NEXT_PUBLIC_SITE_URL": "https://nodepilot.dev"
  },
  "github": {
    "enabled": true,
    "silent": false,
    "autoAlias": true
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "development": true
    }
  }
} 